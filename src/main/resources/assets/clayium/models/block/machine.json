{
  "multipart": [
    {
      "apply": {
        "model": "clayium:block/front_overlay_all"
      }
    },

    {
      "when": { "facing": "north" },
      "apply": {
        "model": "clayium:block/overlay"
      }
    },
    {
      "when": { "facing": "south" },
      "apply": {
        "model": "clayium:block/overlay",
        "y": 180
      }
    },
    {
      "when": { "facing": "west" },
      "apply": {
        "model": "clayium:block/overlay",
        "y": 270
      }
    },
    {
      "when": { "facing": "east" },
      "apply": {
        "model": "clayium:block/overlay",
        "y": 90
      }
    }
  ]
}